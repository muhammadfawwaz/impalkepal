        <input type="text" name="jumlah" id="" value="<% if(obj) { %><%= obj.jumlah %><% } %>"><br>
        <input type="text" name="harga" id="" value="<% if(obj) { %><%= obj.harga %><% } %>"><br>
        <input type="submit" value="Update">
    </form>
    <form action="/admin" method="get">
        <input type="submit" value="Dashboard">
    </form>
</body>
</html>